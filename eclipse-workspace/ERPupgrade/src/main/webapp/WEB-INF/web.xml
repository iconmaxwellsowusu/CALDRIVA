<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>letter</servlet-name>
  	<display-name>letter</display-name>
  	<description></description>
  	<servlet-class>upload.letter</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>download</servlet-name>
  	<display-name>download</display-name>
  	<description></description>
  	<servlet-class>upload.download</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>printer</servlet-name>
  	<display-name>printer</display-name>
  	<description></description>
  	<servlet-class>upload.printer</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>letter</servlet-name>
  	<url-pattern>/letter</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>printer</servlet-name>
  	<url-pattern>/printer</url-pattern>
  </servlet-mapping>
 
</web-app>
